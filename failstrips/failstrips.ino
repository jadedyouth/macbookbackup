const uint8_t kMatrixWidth = 15;
const uint8_t kMatrixHeight = 50;
const bool    kMatrixSerpentineLayout = true;

uint16_t XY( uint8_t y, uint8_t x)
{
  uint16_t i;
  
  if( kMatrixSerpentineLayout == false) {
    i = (y * kMatrixWidth) + x;
  }

  if( kMatrixSerpentineLayout == true) {
    if( y & 0x01) {
      // Odd rows run backwards
      uint8_t reverseX = (kMatrixWidth - 1) - x;
      i = (y * kMatrixWidth) + reverseX;
    } else {
      // Even rows run forwards
      i = (y * kMatrixWidth) + x;
    }
  }
  
  return i;
}


const uint8_t kMatrixWidthA = 15;
const uint8_t kMatrixHeightA = 50;
const bool    kMatrixSerpentineLayoutA = true;

uint16_t XYA( uint8_t y, uint8_t x)
{
  uint16_t i;
  
  if( kMatrixSerpentineLayoutA == false) {
    i = (y * kMatrixWidthA) + x;
  }

  if( kMatrixSerpentineLayoutA == true) {
    if( y & 0x01) {
      // Odd rows run backwards
      uint8_t reverseX = (kMatrixWidthA - 1) - x;
      i = (y * kMatrixWidthA) + reverseX;
    } else {
      // Even rows run forwards
      i = (y * kMatrixWidthA) + x;
    }
  }
  
  return i;
}

